{
    "name": "Python Jupyter Dev Container",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "devcontainer",
    "workspaceFolder": "/workspace",
    "settings": {
      "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
      "ms-python.python",
      "ms-toolsai.jupyter",
      "iterative.vscode-dvc"
    ],
    "remoteUser": "root",
    "forwardPorts": [3030]
  }
  